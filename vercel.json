{
    "version": 2,
    "builds": [
        { "src": "server.js", "use": "@vercel/node" },
        {
            "src": "/dist/index.js",
            "use": "@vercel/node"
        },
        { "src": "web-build", "use": "@vercel/static-build" },
        {
            "src": "/dist/bundled-client.js",
            "use": "@vercel/static"
        },
        {
            "src": "/videos/*",
            "use": "@vercel/static"
        },
        {
            "src": "/dist/web-build/*",
            "use": "@vercel/static"
        }

    ],
  "routes": [
    { "src": "/web/(.*)", "dest": "server.js" },
    { "src": "/stream/(.*)", "dest": "server.js" },
    { "src": "/(.*)", "dest": "/web-build/$1" }  ]
  }